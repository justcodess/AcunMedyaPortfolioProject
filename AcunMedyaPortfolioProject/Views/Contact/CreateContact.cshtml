@model AcunMedyaPortfolioProject.Models.Contact
@{
    ViewBag.Title = "CreateContact";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>İletişim Bilgisi Ekleme Sayfası</h2>

<div class="container">
    <form method="post">
        <label>Telefon</label>
        @Html.TextBoxFor(x => x.Phone, new { @class = "form-control" })
        <br />
        <label>Mail Adresi</label>
        @Html.TextBoxFor(x => x.MailAdress, new { @class = "form-control" })
        <br />
        <label>Adres</label>
        @Html.TextBoxFor(x => x.Address, new { @class = "form-control" })
        <br />
       
        <button class="btn btn-info ">İletişim Bilgilerini Kaydet</button>
    </form>

</div>

